{
  "name": "treetop",
  "version": "0.0.1",
  "description": "Treetop",
  "type": "module",
  "scripts": {
    "compile-smui-theme-light": "smui-theme compile src/treetop/generated/smui.css -i src/theme",
    "compile-smui-theme-dark": "smui-theme compile src/treetop/generated/smui-dark.css -i src/theme/dark",
    "lint": "eslint ./src ./test",
    "lint:fix": "eslint ./src ./test --fix",
    "prettier": "prettier --check ./src ./test",
    "prettier:fix": "prettier --write ./src ./test",
    "build": "concurrently --max-processes 1 \"npm:build:background_script\" \"npm:build:options\" \"npm:build:treetop\" \"npm:build:welcome\"",
    "build:watch": "concurrently \"npm:build:background_script:watch\" \"npm:build:options:watch\" \"npm:build:treetop:watch\" \"npm:build:welcome:watch\"",
    "build:background_script": "vite build --config vite.background_script.config.ts",
    "build:background_script:watch": "vite build --config vite.background_script.config.ts --mode development --watch",
    "build:options": "vite build --config vite.options.config.ts",
    "build:options:watch": "vite build --config vite.options.config.ts --mode development --watch",
    "build:treetop": "vite build --config vite.treetop.config.ts",
    "build:treetop:watch": "vite build --config vite.treetop.config.ts --mode development --watch",
    "build:welcome": "vite build --config vite.welcome.config.ts",
    "build:welcome:watch": "vite build --config vite.welcome.config.ts --mode development --watch",
    "prepare": "npm run compile-smui-theme-light && npm run compile-smui-theme-dark",
    "test": "jest",
    "validate": "svelte-check --no-tsconfig"
  },
  "author": "Max Smolens <msmolens@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@fontsource/inter": "^5.0.5",
    "@fontsource/material-icons": "^5.0.5",
    "@fontsource/news-cycle": "^5.0.5",
    "@smui/button": "^6.1.4",
    "@smui/dialog": "^6.1.4",
    "@smui/icon-button": "^6.1.4",
    "@smui/linear-progress": "^6.1.4",
    "@smui/snackbar": "^6.1.4",
    "@smui/textfield": "^6.1.4",
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@testing-library/user-event": "^14.4.3",
    "@tsconfig/svelte": "^2.0.1",
    "@types/faker": "^5.5.9",
    "@types/lodash-es": "^4.17.8",
    "@types/webextension-polyfill": "^0.9.1",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "babel-jest": "^28.1.1",
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "faker": "^5.5.3",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "lodash-es": "^4.17.21",
    "mockzilla": "^0.14.0",
    "mockzilla-webextension": "^0.15.0",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.26",
    "postcss-url": "^10.1.3",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "rollup-plugin-copy": "^3.4.0",
    "sass": "^1.63.6",
    "smui-theme": "^6.2.0",
    "svelte": "~3.55.1",
    "svelte-check": "^3.0.3",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^5.0.1",
    "ts-jest": "^28.0.5",
    "typescript": "~4.9.5",
    "vite": "^4.4.4",
    "vite-tsconfig-paths": "^4.2.0",
    "webextension-polyfill": "^0.9.0"
  },
  "overrides": {
    "@babel/generator": "7.21.1"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "webExt": {
    "sourceDir": "./dist"
  }
}
