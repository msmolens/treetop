{
  "name": "treetop",
  "version": "0.0.1",
  "description": "Treetop",
  "scripts": {
    "compile-smui-theme-light": "smui-theme compile src/treetop/generated/smui.css -i src/theme",
    "compile-smui-theme-dark": "smui-theme compile src/treetop/generated/smui-dark.css -i src/theme/dark",
    "lint": "eslint ./src ./test",
    "lint:fix": "eslint ./src ./test --fix",
    "prettier": "prettier --check ./src ./test",
    "prettier:fix": "prettier --write ./src ./test",
    "build": "rollup --config",
    "build:dev": "rollup --config -w",
    "prepare": "npm run compile-smui-theme-light && npm run compile-smui-theme-dark",
    "test": "jest",
    "validate": "svelte-check"
  },
  "author": "Max Smolens <msmolens@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@fontsource/inter": "^4.5.10",
    "@fontsource/material-icons": "^4.5.4",
    "@fontsource/news-cycle": "^4.5.5",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-html": "^0.2.4",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@smui/button": "^6.0.0-beta.16",
    "@smui/dialog": "^6.0.0-beta.16",
    "@smui/icon-button": "^6.0.0-beta.16",
    "@smui/linear-progress": "^6.0.0-beta.16",
    "@smui/snackbar": "^6.0.0-beta.16",
    "@smui/textfield": "^6.0.0-beta.16",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/svelte": "^3.1.1",
    "@testing-library/user-event": "^14.1.1",
    "@tsconfig/svelte": "^2.0.1",
    "@types/faker": "^5.5.8",
    "@types/jest": "^27.5.0",
    "@types/lodash-es": "^4.17.5",
    "@types/webextension-polyfill": "^0.8.0",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "babel-jest": "^27.5.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "faker": "^5.5.3",
    "jest": "^27.5.1",
    "lodash-es": "^4.17.21",
    "mockzilla": "^0.12.0",
    "mockzilla-webextension": "^0.13.0",
    "normalize.css": "^8.0.1",
    "postcss": "^8.3.6",
    "postcss-url": "^10.1.3",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "rollup": "^2.72.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.39.2",
    "smui-theme": "^6.0.0-beta.16",
    "svelte": "^3.38.3",
    "svelte-check": "^2.2.3",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.7.4",
    "ts-jest": "^27.1.4",
    "tslib": "^2.3.1",
    "typescript": "^4.1.6",
    "webextension-polyfill": "^0.8.0"
  },
  "engines": {
    "node": ">=14 <17",
    "npm": ">=8"
  },
  "webExt": {
    "sourceDir": "./dist"
  }
}
